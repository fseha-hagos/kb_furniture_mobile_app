{
  "expo": {
    "name": "kb-furniture",
    "slug": "kb-furniture",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo/kb-logo-last.png",
    "scheme": "kbfurniture",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo/kb-furniture-high-resolution-logo-transparent.png",
        "backgroundColor": "#00685C"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fseha.kbfurniture"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/logo/kb-furniture-high-resolution-logo-transparent.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#00685C"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9bafe5ec-bd55-47c6-a676-ed85043a8a77"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9bafe5ec-bd55-47c6-a676-ed85043a8a77"
    }
  }
}
